page{
  background: #f0f0f0;
}
.search__form{
    height: 67rpx; 
    display: block;
    margin: 0 auto;
    position: relative;
    
    width: 90%;
}
.phcolor{  
    color: #ddd;  
}
.topheight{
   padding: 10px 5px;  
   height: 67rpx;
}
.search__top{ padding: 10px 5px; background: #fff;   z-index: 5;}
.search__input{  border-radius:48rpx; overflow: hidden;margin: 0 auto; padding:0 85rpx 0 75rpx; background: #eee; font-size: 14px;height:65rpx;
  line-height:65rpx;}
.search__icon{ position: absolute; left:10rpx;top:18rpx;}
.clearButton{background:none;position:absolute;border:0;right:20rpx;top:6rpx;bottom:0;width:80rpx; outline: 0;border-radius:0;}
.panel{ background: #fff; padding:10px; border-bottom: solid 1px #ddd; border-top:solid 1px #ddd;}
.cell__hd{background-color:#1AAD19;border-radius:7rpx;width:80rpx;height:80rpx;line-height:80rpx;text-align:center; float: left; vertical-align: middle;}
.cell_primary{ float:left; line-height: 80rpx; font-size: 16px;}
.cell__ft{ width:20px; height: 20px; float:right; padding-top: 8px;}
.cell__ft image{ width: 20px; height: 20px;}
.clear{ clear: both;}
.searchData{
  font-size: 29rpx;
  color: #333;
  height: 67rpx; 
  line-height: 67rpx; 
  text-align: center;
  padding:0 !important;
  margin:0 !important;
  width:70rpx !important;
  font-weight: normal;
  background-color:transparent !important;
}


  
.clear{
  clear: both;
  overflow: hidden;
}
 
.tabTit{
  height:90rpx;
  line-height: 90rpx;
  border-bottom: solid 1px #eee;
  
  width: 750rpx;
  z-index: 1;
  background: #fff;
  font-size:15px;
  text-align: center;
}
 
 
 
.tabTit .active{
  color:#e64340;
  background: #fff;
}
 
.tabTit .active image{
  transform: rotate(180deg);
  background: none;
}
 
.tabTit image{
  width:26rpx;
  height:26rpx;
  vertical-align: middle;
  margin-left: 5px;
 
}
 
.tabLayer{
  box-shadow: 0 5px 5px rgba(0,0,0,.15);
  width:750rpx;
  overflow: hidden;
  position: fixed;
  top:90rpx;
  z-index: 1;
  background: #fff;
  padding-bottom: 40rpx;
  border-bottom: solid 1px #eee;
  text-align: center;
}
.tabLayer text{
  width:210rpx;
  height:60rpx;
  line-height:60rpx;
  float:left;
  border: solid 1px #eee;
  margin:20rpx 0 0 26rpx;
  font-size:15px;
}
.tabLayer .active{
  color:#e64340;
  border-color:#e64340;
  }
 
.flex1 {
    overflow: hidden;
    display: block;
    height:56rpx;
    width:151rpx;border-radius: 28rpx;border:1rpx solid #ddd;
    line-height: 56rpx;
    text-align: center;
    margin-left: 20rpx;

}
 
/*弹性盒模型*/
.box {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
 
navigator{
  display:inline;
}
.list{
  height:250rpx;width:94%;margin:0 auto;
  border-radius:10rpx;background:#fff;
}
 
.list .list_item{
  margin-top:10px;
  padding:10px;
  height:100px;
  border-bottom:1px solid #E8E8E8;
}
.list .list_item .img{
  float:left;
  width:40%;
  height:100%;
}
.list .list_item .img image{
   width:100%;
   height:100%;
}
 
.list .list_item .info{
  width:59%;
  float:right;
  height:100px;
  position:relative;
}
.list .list_item .info .title{
  color:#333;
  margin-left:10px;
  font-size: 15px;
}
 
.list .list_item .info .price{
  color:#FF2727;
  margin-left:10px;
  margin-top:10px;
  font-size:15px;
}
 
.list .list_item .info .num{
  position: absolute;
  left:0px;
  bottom:10px;
  color:#747474;
  margin-left:10px;
  font-size:15px;
}
/*订单列表*/
.orderDetails {
  background: #fff;
  margin:20rpx 0;
  border-radius: 10rpx;
}
 
.orderListTitle {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #BEBEBE;
}
 
.orderListTitle .userName {
  padding-right: 50rpx;
  margin-left:10px;
  font-size:26rpx;
  
}
 
.orderListTitle .orderStatus {
  float: right;
  margin-right: 20rpx;
  color: #000;
  font-size: 22rpx;
}
.allnum{margin: 0px 20rpx;
  color: #000;
  font-size: 26rpx;
  text-align: right;
}
.allmoney{color:#ff0006;font-size: 26rpx;}
.productstate{
  width:22rpx;
  height:33rpx;
}
 
.orderListDetails {
  display: flex;
  height: 180rpx;
  background:#fff;
}
 
.orderListDetails .productImg {
  flex: 1;
  height:179rpx;
  width:186px;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

 
.orderListDetails .productInfo {
  flex: 4;
  margin: 40rpx 10rpx 20rpx 30rpx;
 font-size:26rpx;
 width:0%;
}
 .fire{
   width:22rpx;height:33rpx
 }
.orderListDetails .productInfo .productTitle {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom:5rpx;
  font-size:22rpx;
  color:#898989;
}
 
.productCount {
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid #ebebeb;
}

.cover{width:210rpx;height:140rpx;}